import boto3
client = boto3.client('ec2',
                      aws_access_key_id='AKIAS6CPPOVCWVGJGSCF',
                      aws_secret_access_key='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',
                      region_name="ap-northeast-1"
